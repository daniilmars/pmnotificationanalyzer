{
    "_version": "1.58.0",
    "sap.app": {
        "id": "simple.fiori.app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}"
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": { "desktop": true, "tablet": true, "phone": true }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "simple.fiori.app.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.108.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": { "bundleName": "simple.fiori.app.i18n.i18n" }
            }
        }
    }
}
