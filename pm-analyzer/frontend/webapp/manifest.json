{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.sap.pm.pmanalyzerfiori",
        "type": "application",
        "applicationVersion": { "version": "1.0.0" },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": [
                "maintenance",
                "notifications",
                "quality",
                "reliability",
                "SAP PM",
                "AI",
                "analytics"
            ]
        },
        "dataSources": {
            "analysisApi": {
                "uri": "/api/",
                "type": "REST"
            },
            "notificationsMockData": {
                "uri": "./mock_data_en.json",
                "type": "JSON"
            }
        },
        "crossNavigation": {
            "inbounds": {
                "pmanalyzer-display": {
                    "semanticObject": "PMNotification",
                    "action": "analyze",
                    "title": "{{appTitle}}",
                    "subTitle": "{{appSubTitle}}",
                    "icon": "sap-icon://machine",
                    "signature": {
                        "parameters": {
                            "notificationId": {
                                "required": false
                            }
                        },
                        "additionalParameters": "allowed"
                    }
                },
                "pmanalyzer-quality": {
                    "semanticObject": "PMQuality",
                    "action": "dashboard",
                    "title": "{{qualityTitle}}",
                    "icon": "sap-icon://quality-issue",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                },
                "pmanalyzer-reliability": {
                    "semanticObject": "PMReliability",
                    "action": "dashboard",
                    "title": "{{reliabilityTitle}}",
                    "icon": "sap-icon://monitor-payments",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            },
            "outbounds": {
                "displayMaintenanceNotification": {
                    "semanticObject": "MaintenanceNotification",
                    "action": "display"
                },
                "displayEquipment": {
                    "semanticObject": "Equipment",
                    "action": "display"
                },
                "displayMaintenanceOrder": {
                    "semanticObject": "MaintenanceOrder",
                    "action": "display"
                },
                "displayFunctionalLocation": {
                    "semanticObject": "FunctionalLocation",
                    "action": "display"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://machine",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": { "desktop": true, "tablet": true, "phone": true }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.f": {},
                "sap.ushell": {},
                "sap.ui.comp": {},
                "sap.suite.ui.commons": {}
            }
        },
        "contentDensities": { "compact": true, "cozy": true },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.sap.pm.pmanalyzerfiori.i18n.i18n",
                    "supportedLocales": ["en", "de"],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "analysis": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": ""
            },
            "ui": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": ""
            }
        },
        "rootView": {
            "viewName": "com.sap.pm.pmanalyzerfiori.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.sap.pm.pmanalyzerfiori.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "worklist",
                    "pattern": "",
                    "target": "worklist"
                },
                {
                    "name": "object",
                    "pattern": "Notifications/{notificationId}",
                    "target": "object"
                },
                {
                    "name": "qualityDashboard",
                    "pattern": "QualityDashboard",
                    "target": "qualityDashboard"
                },
                {
                    "name": "reliabilityDashboard",
                    "pattern": "ReliabilityDashboard",
                    "target": "reliabilityDashboard"
                },
                {
                    "name": "auditDashboard",
                    "pattern": "AuditDashboard",
                    "target": "auditDashboard"
                }
            ],
            "targets": {
                "worklist": {
                    "viewName": "Worklist",
                    "viewId": "worklist",
                    "viewLevel": 1
                },
                "object": {
                    "viewName": "Object",
                    "viewId": "object",
                    "viewLevel": 2
                },
                "qualityDashboard": {
                    "viewName": "QualityDashboard",
                    "viewId": "qualityDashboard",
                    "viewLevel": 1
                },
                "reliabilityDashboard": {
                    "viewName": "ReliabilityDashboard",
                    "viewId": "reliabilityDashboard",
                    "viewLevel": 1
                },
                "auditDashboard": {
                    "viewName": "AuditDashboard",
                    "viewId": "auditDashboard",
                    "viewLevel": 1
                }
            }
        }
    },
    "sap.flp": {
        "type": "application",
        "config": {}
    },
    "sap.cloud": {
        "public": true,
        "service": "pm-notification-analyzer"
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    }
}
