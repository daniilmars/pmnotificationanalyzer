{
  "welcomeFile": "index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "destination": "pm-analyzer-backend"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "localDir": "resources/pm-analyzer-fiori/dist"
    }
  ]
}